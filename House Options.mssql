SELECT
	HOUSEMASTER.COMPANYCODE,
	HOUSEMASTER.LOTNUMBER,
	HOUSEOPTIONS.OPTIONCODE
FROM BBJ_IHMS_EYA.EYA.HOUSEMASTER HOUSEMASTER,
	BBJ_IHMS_EYA.EYA.HOUSEOPTIONS HOUSEOPTIONS
WHERE HOUSEOPTIONS.COMPANYCODE = HOUSEMASTER.COMPANYCODE AND
	HOUSEOPTIONS.DEVELOPMENTCODE = HOUSEMASTER.DEVELOPMENTCODE AND
	HOUSEOPTIONS.HOUSENUMBER = HOUSEMASTER.HOUSENUMBER AND
	HOUSEMASTER.COMPANYCODE>='839'
	